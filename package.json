{
	"name": "utilitydust",
	"version": "0.0.0",
	"description": "A customizable Discord general-purpose bot, written in TypeScript",
	"main": "dist/index.js",
	"engines": {
		"node": ">=18.x"
	},
	"scripts": {
		"start": "node dist/index.js",
		"build": "npm run clean && npm run tsc && npm run tsc-alias",
		"clean": "rimraf dist",
		"tsc": "tsc --build tsconfig.json",
		"tsc-alias": "tsc-alias -p tsconfig.json",
		"deploy": "npm run build && npm run start",
		"dev": "npx nodemon --exec \"npm run deploy\" -e \"ts\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GaryCraft/UtilityDust.git"
	},
	"author": "GaryCraft",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/GaryCraft/UtilityDust/issues"
	},
	"homepage": "https://github.com/GaryCraft/UtilityDust#readme",
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/node": "^17.0.25",
		"@types/node-schedule": "^2.1.0",
		"@typescript-eslint/eslint-plugin": "^5.29.0",
		"@typescript-eslint/parser": "^5.29.0",
		"eslint": "^8.13.0",
		"sqlite3": "^5.0.8",
		"tsc-alias": "^1.8.2",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"@haileybot/captcha-generator": "^1.7.0",
		"@spaceproject/findrecursive": "^1.0.4",
		"discord.js": "^14.7.1",
		"express": "^4.18.1",
		"mysql2": "^2.3.3",
		"node-schedule": "^2.1.1",
		"parzival": "^0.2.5-0",
		"reflect-metadata": "^0.1.13",
		"typeorm": "^0.3.6",
		"uuid": "^8.3.2",
		"winston": "^3.7.2"
	}
}
